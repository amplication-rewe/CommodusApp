{
  "name": "angular-fe",
  "version": "0.1.0-DEV",
  "private": true,
  "license": "MIT",
  "scripts": {
    "prepare": "husky install",
    "build": "if [ -z ${CI_MERGE_REQUEST_ID} ]; then nx build --base-href=/riag/tech/team-ca/commodus-app/angular-fe/; else nx build --base-href=/riag/tech/team-ca/commodus-app/angular-fe-mr-${CI_MERGE_REQUEST_ID}/; fi",
    "postbuild": "npm-run-all postbuild:*",
    "postbuild:replace": "node tools/node/replace.js",
    "postbuild:preload": "node tools/node/preload.js",
    "lint": "npm-run-all --parallel lint:*",
    "lint:scripts": "nx run web-ui:lint",
    "lint:styles": "nx run web-ui:stylelint",
    "start": "nx run web-ui:serve:development",
    "test:unit": "nx run web-ui:test",
    "test:unit:coverage": "nx run web-ui:test --coverage --output-style=static",
    "test:ui": "nx run web-ui-test:test-ui",
    "test:ui:dev": "nx run web-ui-test:test-ui --browser chrome --watch",
    "format": "prettier -w 'apps/**/*.{html,scss,ts,json}'",
    "preinstall": "npx only-allow pnpm",
    "update:nx": "npx nx migrate latest",
    "update:ng": "FORCE_NG_UPDATE=true nx migrate @angular/cli@latest && nx migrate @angular/core@latest && nx migrate @angular/material@latest",
    "ci": "npm-run-all --parallel lint test:unit:coverage test:ui build"
  },
  "dependencies": {
    "@angular/animations": "17.0.7",
    "@angular/cdk": "17.0.4",
    "@angular/common": "17.0.7",
    "@angular/compiler": "17.0.7",
    "@angular/core": "17.0.7",
    "@angular/forms": "17.0.7",
    "@angular/material": "17.0.4",
    "@angular/platform-browser": "17.0.7",
